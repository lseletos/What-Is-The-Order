            </intent-filter>
        </activity>
        <activity 
            android:name="GameInfo"
            android:screenOrientation="landscape">
        </activity>
        <activity 
            android:name="TimeGame"
            android:screenOrientation="landscape">            
        </activity>
        <activity 
            android:name="GameInstructions"
            android:screenOrientation="landscape">            
        </activity>
        <activity android:name="ViewReport" 
            android:screenOrientation="landscape">
            
        </activity>
        <activity android:name="GoodJobScreen"
            android:screenOrientation="landscape">
            
        </activity>

            
    </application>

</manifest>

